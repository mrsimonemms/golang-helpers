{
  "name": "devcontainer",
  "image": "ghcr.io/mrsimonemms/devcontainers/full",
  "features": {
    "ghcr.io/mrsimonemms/devcontainers/cobra-cli:1": {},
    "ghcr.io/guiyomh/features/goreleaser:0": {},
    "ghcr.io/azutake/devcontainer-features/go-packages-install:0": {
      "PACKAGES": "google.golang.org/protobuf/cmd/protoc-gen-go@latest,google.golang.org/grpc/cmd/protoc-gen-go-grpc@latest"
    },
    "ghcr.io/devcontainers-extra/features/protoc:1": {},
    "ghcr.io/mrsimonemms/devcontainers/buf:1": {},
    "ghcr.io/devcontainers-extra/features/grpcurl-asdf:2": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "zxh404.vscode-proto3"
      ],
      "settings": {}
    }
  },
  "containerEnv": {
    "GIT_REPO": "github.com/mrsimonemms/golang-helpers"
  }
}
